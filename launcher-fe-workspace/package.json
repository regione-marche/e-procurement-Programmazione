{
  "name": "launcher-fe-workspace",
  "version": "1.1.3",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "yarn run preset:customer --project app-launcher && yarn run postcss --project app-launcher && yarn run ng serve --project app-launcher --port 8100 --live-reload false",
    "build": "yarn run preset:customer --project app-launcher --customer default --projectType sa --environmentType production && yarn run postcss --project app-launcher && yarn run ng build --project app-launcher --configuration production --base-href ./ && yarn run minify:json --project app-launcher",
    "assets": "yarn run preset:customer --project app-launcher && yarn run postcss --project app-launcher && yarn run compile:assets --project app-launcher && yarn run minify:json --project app-launcher",
    "postcss": "yarn run node scripts/postcss.js",
    "minify:json": "yarn run node scripts/minify-json.js",
    "preset:customer": "yarn run node scripts/preset-customer.js",
    "compile:assets": "yarn run node scripts/compile-assets.js",
    "clean": "rimraf dist",
    "node": "node --max-old-space-size=4096",
    "ng": "yarn run node node_modules/@angular/cli/bin/ng",
    "build:all-customers": "node scripts/build-all-customers.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.7",
    "@angular/cdk": "^17.3.7",
    "@angular/common": "^17.3.7",
    "@angular/compiler": "^17.3.7",
    "@angular/core": "^17.3.7",
    "@angular/elements": "^17.3.7",
    "@angular/forms": "^17.3.7",
    "@angular/platform-browser": "^17.3.7",
    "@angular/platform-browser-dynamic": "^17.3.7",
    "@angular/router": "^17.3.7",
    "@fortawesome/fontawesome-free": "5.15.4",
    "@maggioli-design-system/icons": "1.70.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@webcomponents/custom-elements": "^1.6.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "angular-oauth2-oidc": "~17.0.2",
    "friendly-challenge": "^0.9.17",
    "hash-sum": "^2.0.0",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "ngx-toastr": "^18.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^17.16.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular/cli": "^17.3.6",
    "@angular/compiler-cli": "^17.3.7",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.12.10",
    "chalk": "^4.1.2",
    "codelyzer": "6.0.2",
    "cssnano": "^6.0.1",
    "http-server": "^14.1.1",
    "postcss": "^8.4.29",
    "protractor": "~7.0.0",
    "readline": "^1.3.0",
    "rimraf": "^6.0.1",
    "sass": "^1.66.1",
    "shelljs": "^0.8.5",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~5.4.5",
    "yargs": "^17.7.2"
  }
}